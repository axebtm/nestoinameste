        function addHoverPointerToSvg(svg,onClick) {
            svg.style['fill'] = '#000';
            svg.style['fill-opacity'] = '0.0';
            const actions = [['mouseover','pointer'],['mouseout','inherit'],['click']];
            actions.forEach( action => {
                svg.addEventListener(action[0], e => {
                    if (action[1]) {
                        document.body.style.cursor = action[1]
                    } else {
                        onClick();
                    }
                });
            });
        }

        
        
        

    
